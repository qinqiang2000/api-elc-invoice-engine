<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kingdee.fpy.mapper.SecUsercontactMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.kingdee.fpy.model.SecUsercontact">
        <id column="fentryid" jdbcType="BIGINT" property="entryid" />
        <result column="fid" jdbcType="BIGINT" property="id" />
        <result column="fseq" jdbcType="BIGINT" property="seq" />
        <result column="fcontact" jdbcType="VARCHAR" property="contact" />
        <result column="fcontacttypeid" jdbcType="BIGINT" property="contacttypeid" />
        <result column="fisdefault" jdbcType="CHAR" property="isdefault" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        fentryid, fid, fseq, fcontact, fcontacttypeid, fisdefault
    </sql>

    <!-- 根据主键查询 -->
    <select id="selectByPrimaryKey" resultMap="BaseResultMap">
        SELECT
        <include refid="Base_Column_List" />
        FROM t_sec_usercontact
        WHERE fentryid = #{entryid}
    </select>

    <!-- 查询所有记录 -->
    <select id="selectAll" resultMap="BaseResultMap">
        SELECT
        <include refid="Base_Column_List" />
        FROM t_sec_usercontact
        ORDER BY fentryid DESC
    </select>

    <!-- 根据条件查询 -->
    <select id="selectByCondition" resultMap="BaseResultMap">
        SELECT
        <include refid="Base_Column_List" />
        FROM t_sec_usercontact
        <where>
            <if test="condition.entryid != null">
                AND fentryid = #{condition.entryid}
            </if>
            <if test="condition.id != null">
                AND fid = #{condition.id}
            </if>
            <if test="condition.seq != null">
                AND fseq = #{condition.seq}
            </if>
            <if test="condition.contact != null">
                AND fcontact = #{condition.contact}
            </if>
            <if test="condition.contacttypeid != null">
                AND fcontacttypeid = #{condition.contacttypeid}
            </if>
            <if test="condition.isdefault != null">
                AND fisdefault = #{condition.isdefault}
            </if>
        </where>
        ORDER BY fentryid DESC
    </select>

    <!-- 根据条件统计数量 -->
    <select id="countByCondition" resultType="int">
        SELECT COUNT(*)
        FROM t_sec_usercontact
        <where>
            <if test="condition.entryid != null">
                AND fentryid = #{condition.entryid}
            </if>
            <if test="condition.id != null">
                AND fid = #{condition.id}
            </if>
            <if test="condition.seq != null">
                AND fseq = #{condition.seq}
            </if>
            <if test="condition.contact != null">
                AND fcontact = #{condition.contact}
            </if>
            <if test="condition.contacttypeid != null">
                AND fcontacttypeid = #{condition.contacttypeid}
            </if>
            <if test="condition.isdefault != null">
                AND fisdefault = #{condition.isdefault}
            </if>
        </where>
    </select>

    <!-- 插入记录 -->
    <insert id="insert" useGeneratedKeys="true" keyProperty="entryid">
        INSERT INTO t_sec_usercontact
        (fentryid, fid, fseq, fcontact, fcontacttypeid, fisdefault)
        VALUES
        (#{entryid}, #{id}, #{seq}, #{contact}, #{contacttypeid}, #{isdefault})
    </insert>

    <!-- 选择性插入记录 -->
    <insert id="insertSelective" useGeneratedKeys="true" keyProperty="entryid">
        INSERT INTO t_sec_usercontact
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="entryid != null">
                fentryid,
            </if>
            <if test="id != null">
                fid,
            </if>
            <if test="seq != null">
                fseq,
            </if>
            <if test="contact != null">
                fcontact,
            </if>
            <if test="contacttypeid != null">
                fcontacttypeid,
            </if>
            <if test="isdefault != null">
                fisdefault,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="entryid != null">
                #{entryid},
            </if>
            <if test="id != null">
                #{id},
            </if>
            <if test="seq != null">
                #{seq},
            </if>
            <if test="contact != null">
                #{contact},
            </if>
            <if test="contacttypeid != null">
                #{contacttypeid},
            </if>
            <if test="isdefault != null">
                #{isdefault},
            </if>
        </trim>
    </insert>

    <!-- 根据主键更新 -->
    <update id="updateByPrimaryKey">
        UPDATE t_sec_usercontact
        SET fid = #{id},
            fseq = #{seq},
            fcontact = #{contact},
            fcontacttypeid = #{contacttypeid},
            fisdefault = #{isdefault}
        WHERE fentryid = #{entryid}
    </update>

    <!-- 根据主键选择性更新 -->
    <update id="updateByPrimaryKeySelective">
        UPDATE t_sec_usercontact
        <set>
            <if test="id != null">
                fid = #{id},
            </if>
            <if test="seq != null">
                fseq = #{seq},
            </if>
            <if test="contact != null">
                fcontact = #{contact},
            </if>
            <if test="contacttypeid != null">
                fcontacttypeid = #{contacttypeid},
            </if>
            <if test="isdefault != null">
                fisdefault = #{isdefault},
            </if>
        </set>
        WHERE fentryid = #{entryid}
    </update>

    <!-- 根据主键删除 -->
    <delete id="deleteByPrimaryKey">
        DELETE FROM t_sec_usercontact
        WHERE fentryid = #{entryid}
    </delete>

    <!-- 批量删除 -->
    <delete id="deleteByIds">
        DELETE FROM t_sec_usercontact
        WHERE fentryid IN
        <foreach item="id" collection="ids" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>
